<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.stressless.stressless">

    <!--
    @mipmap/ic_launcher
    @mipmap/ic_launcher_round
    -->
    <application
        android:allowBackup="true"
        android:icon="@drawable/stress"
        android:label="@string/app_name"
        android:roundIcon="@drawable/stress"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activity.LoginActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.PrincipalActivity" />
        <activity android:name=".Activity.CadastroActivity" />
        <activity android:name=".Activity.SesnoresActivity" />
        <activity android:name=".Activity.ManuaisActivity" />
        <activity android:name=".Activity.EscolhaActivity" />
        <activity android:name=".Activity.Sensores_ListActivity" />
        <activity android:name=".Activity.Manuais_ListActivity" />
        <activity android:name=".Activity.VerActivity"></activity>
    </application>

</manifest>